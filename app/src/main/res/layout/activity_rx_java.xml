<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.chen.myapplication.frame.rxjava.RxJavaActivity">

    <TextView
            android:text="TextView"
            android:layout_width="match_parent" android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"   android:id="@+id/textView3"/>
    <TextView
            android:text="TextView" android:gravity="center"
            android:layout_width="match_parent" android:layout_centerHorizontal="true"
            android:layout_height="wrap_content" android:layout_below="@+id/textView3"
            android:layout_alignEnd="@+id/textView3"  android:id="@+id/textView4"/>
    <TextView
            android:text="TextView" android:gravity="center"
            android:layout_width="match_parent" android:layout_centerHorizontal="true"
            android:layout_height="wrap_content" android:layout_below="@+id/textView4"
            android:layout_alignStart="@+id/textView4"  android:id="@+id/textView5"/>
</RelativeLayout>
